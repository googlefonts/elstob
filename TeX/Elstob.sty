\def\adjustopsz#1#2{%
\directlua{
    originalval = #1
    adjustment  = #2
    adjusted = originalval + adjustment
    if adjusted > 18 then adjusted = 18 end
    if adjusted < 6 then adjusted = 6 end
    tex.sprint(adjusted)
}}

