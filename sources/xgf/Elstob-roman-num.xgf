  </gl>

  <gl pnm="nine.lf" init-graphics="no">
    <callg pnm="nine"/>
  </gl>

  <gl pnm="zero.lfzero" init-graphics="no">
    <callg pnm="zero">
      <wpm nm="offset" val="4"/>
      <wpm nm="include-iup" val="-1"/>
    </callg>
    <ip>
      <ref>
	<pt n="12"/>
	<pt n="4"/>
      </ref>
      <pt n="0"/>
      <pt n="1"/>
      <pt n="2"/>
      <pt n="3"/>
    </ip>
    <iup axis="y"/>
  </gl>

  <gl pnm="zero.osf">
    <pm nm="offset" val="0"/>
    <pm nm="include-iup" val="0"/>
    <cn nm="top" val="8"/>
    <cn nm="top-ins" val="24"/>
    <cn nm="bot" val="0"/>
