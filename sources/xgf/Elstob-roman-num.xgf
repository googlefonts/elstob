<?xml version="1.0" encoding="UTF-8"?>
<xgridfit xmlns="http://xgridfit.sourceforge.net/Xgridfit2"
	  xmlns:xi="http://www.w3.org/2001/XInclude">

  <gl pnm="zero">
    <pm nm="offset" val="0"/>
    <pm nm="include-iup" val="0"/>
    <cn nm="top" val="8"/>
    <cn nm="top-ins" val="27"/>
    <cn nm="bot" val="0"/>
    <cn nm="bot-ins" val="17"/>
    <setvs axis="y"/>
    <mv di="baseline-undershoot">
      <pt n="bot"/>
      <mv col="b">
	<pt n="bot-ins"/>
      </mv>
    </mv>
    <mv di="num-top-round">
      <pt n="top"/>
      <mv col="b">
	<pt n="top-ins"/>
      </mv>
    </mv>
    <compile-if test="include-iup = 0">
      <iup axis="y"/>
    </compile-if>
  </gl>

  <gl pnm="one">
    <cn nm="top" val="8"/>
    <cn nm="stem-top" val="1"/>
    <cn nm="stem-bot" val="0"/>
    <cn nm="serif" val="14"/>
    <setvs axis="y"/>
    <push>stem-top top</push>
    <command nm="MDAP" modifier="0"/>
    <command nm="MDAP" modifier="0"/>
    <callm nm="serif-sh">
      <wpm nm="pt-a" val="serif"/>
      <wpm nm="pt-b" val="serif + 1"/>
      <wpm nm="sh-a" val="stem-bot"/>
      <wpm nm="cv-h" val="lc-serif"/>
    </callm>
    <iup axis="y"/>
  </gl>

  <gl pnm="two">
    <cn nm="top" val="19"/>
    <cn nm="top-ins" val="12"/>
    <cn nm="bot" val="34"/>
    <cn nm="bot-ins" val="28"/>
    <setvs axis="y"/>
    <mv di="num-top-round">
      <pt n="top"/>
      <mv col="b">
	<pt n="top-ins"/>
      </mv>
    </mv>
    <callm nm="one-cv-stem">
      <wpm nm="pt1" val="bot"/>
      <wpm nm="pt2" val="bot-ins"/>
    </callm>
    <iup axis="y"/>
  </gl>

  <gl pnm="three">
    <cn nm="top" val="39"/>
    <cn nm="top-ins" val="32"/>
    <cn nm="bot" val="0"/>
    <cn nm="bot-ins" val="13"/>
    <cn nm="mid" val="21"/>
    <set nm="mid-sh">
      <pt n="24"/>
      <pt n="25"/>
      <pt n="47"/>
      <pt n="48"/>
    </set>
    <setvs axis="y"/>
    <mv di="num-top-round">
      <pt n="top"/>
      <mv col="b">
	<pt n="top-ins"/>
      </mv>
    </mv>
    <mv di="baseline-undershoot">
      <pt n="bot"/>
      <mv col="b">
	<pt n="bot-ins"/>
      </mv>
    </mv>
    <ip>
      <ref>
	<pt n="top"/>
	<pt n="bot"/>
      </ref>
      <pt n="mid"/>
    </ip>
    <callm nm="round-point">
      <wpm nm="p" val="mid"/>
    </callm>
    <sh><set ref="mid-sh"/></sh>
    <iup axis="y"/>
  </gl>

  <gl pnm="four">
    <cn nm="diag-top" val="8"/>
    <cn nm="diag-bot-a" val="1"/>
    <cn nm="diag-bot-b" val="0"/>
    <cn nm="bar-top"  val="11"/>
    <cn nm="stem-top" val="20"/>
    <cn nm="stem-bot" val="19"/>
    <cn nm="serif" val="14"/>
    <setvs axis="y"/>
    <callm nm="round-point">
      <wpm nm="p" val="diag-top"/>
    </callm>
    <callm nm="serif-sh">
      <wpm nm="pt-a" val="serif"/>
      <wpm nm="pt-b" val="serif + 1"/>
      <wpm nm="sh-a" val="stem-bot"/>
      <wpm nm="cv-h" val="lc-serif"/>
    </callm>
    <srp whichpointer="2">
      <pt n="diag-top"/>
    </srp>
    <ip>
      <pt n="stem-top"/>
      <pt n="diag-bot-b"/>
    </ip>
    <callm nm="round-point">
      <wpm nm="p" val="stem-top"/>
    </callm>
    <callm nm="plain-stem">
      <wpm nm="pt1" val="diag-bot-b"/>
      <wpm nm="pt2" val="bar-top"/>
    </callm>
    <sh><pt n="diag-bot-a"/></sh>
    <iup axis="y"/>
  </gl>

  <gl pnm="five">
    <cn nm="top" val="26"/>
    <cn nm="top-ins" val="28"/>
    <cn nm="bot" val="0"/>
    <cn nm="bot-ins" val="13"/>
    <cn nm="mid-lower" val="21"/>
    <cn nm="mid-upper" val="36"/>
    <setvs axis="y"/>
    <callm nm="one-cv-stem">
      <wpm nm="pt1" val="top"/>
      <wpm nm="pt2" val="top-ins"/>
      <wpm nm="cv" val="num-top-flat"/>
    </callm>
    <callm nm="one-cv-stem">
      <wpm nm="pt1" val="bot"/>
      <wpm nm="pt2" val="bot-ins"/>
      <wpm nm="cv" val="baseline-undershoot"/>
    </callm>
    <callm nm="mid-stem">
      <wpm nm="ref1" val="top"/>
      <wpm nm="ref2" val="bot"/>
      <wpm nm="pt1" val="mid-lower"/>
      <wpm nm="pt2" val="mid-upper"/>
    </callm>
    <iup axis="y"/>
  </gl>

  <gl pnm="six">
    <cn nm="top" val="9"/>
    <cn nm="bot" val="0"/>
    <cn nm="bot-ins" val="19"/>
    <cn nm="mid-lower" val="26"/>
    <cn nm="mid-upper" val="34"/>
    <setvs axis="y"/>
    <push>(top + 1) top</push>
    <command nm="MDAP" modifier="0"/>
    <command nm="MDAP" modifier="0"/>
    <callm nm="one-cv-stem">
      <wpm nm="pt1" val="bot"/>
      <wpm nm="pt2" val="bot-ins"/>
      <wpm nm="cv" val="baseline-undershoot"/>
    </callm>
    <callm nm="mid-stem">
      <wpm nm="ref1" val="top"/>
      <wpm nm="ref2" val="bot"/>
      <wpm nm="pt1" val="mid-lower"/>
      <wpm nm="pt2" val="mid-upper"/>
    </callm>
    <iup axis="y"/>
  </gl>

  <gl pnm="seven" init-graphics="no">
    <setvs axis="y"/>
    <callm nm="one-cv-stem">
      <wpm nm="pt1" val="13"/>
      <wpm nm="pt2" val="6"/>
      <wpm nm="cv" val="num-top-flat"/>
    </callm>
    <push>0 baseline</push>
    <command nm="MIAP" modifier="1"/>
    <iup axis="y"/>
  </gl>

  <gl pnm="eight" init-graphics="no">
    <setvs axis="y"/>
    <callm nm="one-cv-stem">
      <wpm nm="pt1" val="43"/>
      <wpm nm="pt2" val="15"/>
      <wpm nm="cv" val="num-top-round"/>
    </callm>
    <callm nm="one-cv-stem">
      <wpm nm="pt1" val="0"/>
      <wpm nm="pt2" val="30"/>
      <wpm nm="cv" val="baseline-undershoot"/>
    </callm>
    <iup axis="y"/>
  </gl>

  <gl pnm="nine" init-graphics="no">
    <cn nm="top" val="33"/>
    <cn nm="top-ins" val="10"/>
    <cn nm="bot" val="0"/>
    <cn nm="mid-lower" val="25"/>
    <cn nm="mid-upper" val="17"/>
    <setvs axis="y"/>
    <push>(bot + 1) bot</push>
    <command nm="MDAP" modifier="0"/>
    <command nm="MDAP" modifier="0"/>
    <callm nm="one-cv-stem">
      <wpm nm="pt1" val="top"/>
      <wpm nm="pt2" val="top-ins"/>
      <wpm nm="cv" val="num-top-round"/>
    </callm>
    <callm nm="mid-stem">
      <wpm nm="ref1" val="top"/>
      <wpm nm="ref2" val="bot"/>
      <wpm nm="pt1" val="mid-lower"/>
      <wpm nm="pt2" val="mid-upper"/>
    </callm>
    <iup axis="y"/>
  </gl>

  <gl pnm="zero.zero" init-graphics="no">
    <callg pnm="zero">
      <wpm nm="offset" val="4"/>
      <wpm nm="include-iup" val="-1"/>
    </callg>
    <ip>
      <ref>
	<pt n="12"/>
	<pt n="4"/>
      </ref>
      <pt n="0"/>
      <pt n="1"/>
      <pt n="2"/>
      <pt n="3"/>
    </ip>
    <iup axis="y"/>
  </gl>

  <gl pnm="zero.lf" init-graphics="no">
    <callg pnm="zero"/>
  </gl>

  <gl pnm="one.lf" init-graphics="no">
    <callg pnm="one"/>
  </gl>

  <gl pnm="two.lf" init-graphics="no">
    <callg pnm="two"/>
  </gl>

  <gl pnm="three.lf" init-graphics="no">
    <callg pnm="three"/>
  </gl>

  <gl pnm="four.lf" init-graphics="no">
    <callg pnm="four"/>
  </gl>

  <gl pnm="five.lf" init-graphics="no">
    <callg pnm="five"/>
  </gl>

  <gl pnm="six.lf" init-graphics="no">
