    <setvs axis="y"/>
    <callm nm="lc-horz-stroke-cv">
      <wpm nm="pt1" val="bot"/>
      <wpm nm="pt2" val="bot-ins"/>
      <wpm nm="cv" val="xheight-overshoot"/>
      <wpm nm="cv-h" val="zero-osf-stem"/>
    </callm>
    <callm nm="lc-horz-stroke-cv">
      <wpm nm="pt1" val="top"/>
      <wpm nm="pt2" val="top-ins"/>
      <wpm nm="cv" val="baseline-undershoot"/>
      <wpm nm="cv-h" val="zero-osf-stem"/>
    </callm>
    <compile-if test="include-iup = 0">
      <iup axis="y"/>
    </compile-if>
  </gl>

  <gl pnm="one.osf">
    <setvs axis="y"/>
    <callm nm="serif-sh">
      <wpm nm="pt-a" val="4"/>
      <wpm nm="pt-b" val="5"/>
      <wpm nm="sh-a" val="0"/>
      <wpm nm="cv-h" val="lc-serif"/>
    </callm>
    <callm nm="serif-sh">
      <wpm nm="pt-a" val="11"/>
      <wpm nm="pt-b" val="10"/>
      <wpm nm="sh-a" val="1"/>
      <wpm nm="cv" val="xheight"/>
      <wpm nm="cv-h" val="lc-serif"/>
    </callm>
    <iup axis="y"/>
  </gl>

  <gl pnm="two.osf">
    <setvs axis="y"/>
    <callm nm="one-cv-stem">
      <wpm nm="pt1" val="18"/>
      <wpm nm="pt2" val="9"/>
      <wpm nm="cv" val="xheight-overshoot"/>
    </callm>
    <callm nm="one-cv-stem">
      <wpm nm="pt1" val="32"/>
      <wpm nm="pt2" val="28"/>
      <wpm nm="cv" val="baseline"/>
    </callm>
    <iup axis="y"/>
  </gl>

  <gl pnm="three.osf">
    <setvs axis="y"/>
    <callm nm="one-cv-stem">
