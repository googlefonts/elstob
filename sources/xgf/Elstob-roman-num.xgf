  </gl>

  <gl pnm="six.osf">
    <cn nm="top" val="10"/>
    <cn nm="bot" val="0"/>
    <cn nm="bot-ins" val="21"/>
    <cn nm="mid-lower" val="29"/>
    <cn nm="mid-upper" val="37"/>
