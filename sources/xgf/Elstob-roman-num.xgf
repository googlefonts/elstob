    <sh ref-ptr="1"><pt n="10"/></sh>
    <sh ref-ptr="2"><pt n="8"/></sh>
    <push>0</push>
    <command nm="MDAP" modifier="1"/>
    <iup axis="y"/>
  </gl>

  <gl pnm="eight.osf">
