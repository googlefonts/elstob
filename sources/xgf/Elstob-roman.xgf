      <wpm nm="pt-b" val="serif + 1"/>
      <wpm nm="sh-a" val="right-stem-bot"/>
    </callm>
    <iup axis="y"/>
  </gl>

  <gl pnm="uniA784"> <!-- Sinsular -->
    <cn nm="stem-top" val="8"/>
    <cn nm="arm-top" val="20"/>
    <cn nm="arm-top-ins" val="34"/>
    <cn nm="arm-end" val="28"/>
    <cn nm="stem-bot" val="0"/>
    <setvs axis="y"/>
    <mv di="cap-height-overshoot">
      <pt n="stem-top"/>
      <sh>
	<pt n="stem-top - 1"/>
	<pt n="stem-top - 2"/>
	<pt n="arm-top"/>
      </sh>
    </mv>
    <mv round="yes">
      <pt n="arm-top"/>
      <mv color="black">
	<pt n="arm-top-ins"/>
      </mv>
      <mv round="yes">
	<pt n="arm-end"/>
      </mv>
    </mv>
    <mv>
      <pt n="stem-bot"/>
    </mv>
    <iup axis="y"/>
  </gl>

  <gl pnm="uniA786"> <!-- Tinsular -->
    <cn nm="top" val="48"/>
    <cn nm="top-under" val="49"/>
    <cn nm="curve-top" val="8"/>
    <cn nm="bot" val="0"/>
    <cn nm="bot-ins" val="17"/>
    <set nm="top-sh">
      <pt n="43"/>
      <pt n="34"/>
    </set>
    <setvs axis="y"/>
    <mv di="cap-height">
      <pt n="top"/>
      <sh><set ref="top-sh"/></sh>
      <mv color="black">
	<pt n="top-under"/>
	<sh><pt n="curve-top"/></sh>
      </mv>
    </mv>
    <mv di="cap-baseline-undershoot">
      <pt n="bot"/>
      <mv di="cap-curved-stem">
	<pt n="bot-ins"/>
      </mv>
    </mv>
    <iup axis="y"/>
  </gl>

