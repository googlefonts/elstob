  <prep>
    <push>4 511</push>
    <command nm="SCANCTRL"/>
    <command nm="SCANTYPE"/>
    <round val="lc-serif"/>
    <round val="lc-bar"/>
    <round val="lc-curved-stem"/>
    <round val="lc-ascender-top"/>
    <round val="lower-f-top"/>
    <round val="lc-ascender-top-slope"/>
    <round val="lc-ascender-top-end"/>
    <round val="xheight-overshoot"/>
    <if test="pixels-per-em &lt; 53">
      <setcv nm="lower-f-top" unit="pixel"
	     val="control-value(lc-ascender-top)"/>
    </if>
    <if test="pixels-per-em &lt; 39">
      <setcv nm="xheight" unit="pixel"
	     val="control-value(xheight-overshoot)"/>
      <setcv nm="baseline-undershoot" unit="pixel"
	     val="control-value(baseline)"/>
      <else>
	<round val="xheight"/>
	<round val="baseline-undershoot"/>
      </else>
    </if>
    <if test="pixels-per-em &lt; 37">
      <setcv nm="cap-bar" unit="pixel"
	     val="control-value(cap-serif)"/>
    </if>
  </prep>

  <!--
      At end, RP0 = p and RP1 = p.
  -->
  <mo nm="round-point">
    <pm nm="p"/>
    <push>p</push>
    <command nm="MDAP" modifier="1"/>
  </mo>

  <mo nm="hook">
    <pm nm="init"/>
    <pm nm="cv" val="-1"/>
    <pm nm="sh"/>
    <pm nm="mv"/>
    <compile-if test="cv &gt;= 0">
      <push>mv sh init cv</push>
      <command nm="MIAP" modifier="1"/>
      <command nm="SHP" modifier="1"/>
      <command nm="MDRP">
	<modifier type="color" val="black"/>
      </command>
      <else>
	<push>mv sh init</push>
	<command nm="MDAP" modifier="1"/>
	<command nm="SHP" modifier="1"/>
	<command nm="MDRP">
	  <modifier type="color" val="black"/>
	</command>
      </else>
    </compile-if>
  </mo>

  <mo nm="lc-bottom-serif">
    <pm nm="bottom-left"/>
    <pm nm="top-left"/>
    <pm nm="cv" val="baseline"/>
    <pm nm="codefrag"/>
    <mv di="cv">
      <pt n="bottom-left"/>
      <mv di="lc-serif" color="black">
	<pt n="top-left"/>
      </mv>
    </mv>
    <callp nm="codefrag"/>
  </mo>

  <!--
      A function, since the parameters aren't complex.
      For each serif, it is assumed that there's one other
      point that needs to be shifted. If the setup is more
      complex, use serif-sh.
  -->
  <fn nm="cap-dbl-serif">
    <pm nm="first-left"/>
    <pm nm="second-left"/>
    <pm nm="first-right"/>
    <pm nm="second-right"/>
    <pm nm="stem1"/>
    <pm nm="stem2"/>
    <pm nm="cv" val="baseline"/>
    <mv di="cv">
      <pt n="first-left"/>
      <sh>
	<pt n="first-right"/>
	<pt n="stem1"/>
	<pt n="stem2"/>
      </sh>
      <mv di="cap-serif" color="black">
	<pt n="second-left"/>
	<sh><pt n="second-right"/></sh>
      </mv>
    </mv>
  </fn>

  <fn nm="f-lig">
    <pm nm="serif-bot"/>
    <pm nm="vertical-bot"/>
    <pm nm="top"/>
    <pm nm="top-ins"/>
    <pm nm="bar-top" val="-1"/>
    <pm nm="bar-bot" val="-1"/>
    <pm nm="bar-bot-left" val="-1"/>
    <pm nm="top-cv" val="lc-ascender-top"/>

    <mv di="baseline">
      <pt n="serif-bot"/>
      <sh>
	<pt n="vertical-bot"/>
      </sh>
      <mv di="lc-serif" color="black">
	<pt n="serif-bot + 1"/>
      </mv>
    </mv>
    <mv di="top-cv">
      <pt n="top"/>
      <mv di="lc-curved-stem" color="black">
	<pt n="top-ins"/>
      </mv>
    </mv>
    <if test="bar-top &gt;= 0">
      <ip>
	<ref>
	  <pt n="serif-bot"/>
	  <pt n="top"/>
	</ref>
	<pt n="bar-top"/>
      </ip>
      <push>bar-bot-left bar-bot bar-top</push>
      <command nm="MDAP" modifier="1"/>
      <command nm="MDRP">
	<modifier type="color" val="black"/>
      </command>
      <command nm="SHP" modifier="0"/>
    </if>
  </fn>

  <fn nm="longs-lig">
    <pm nm="serif-bot"/>
    <pm nm="vertical-bot"/>
    <pm nm="top"/>
    <pm nm="top-ins"/>
    <pm nm="bar-bot"/>
    <pm nm="bar-top"/>
    <pm nm="top-cv" val="lc-ascender-top"/>
    <mv di="baseline">
      <pt n="serif-bot"/>
      <sh>
	<pt n="vertical-bot"/>
      </sh>
      <mv di="lc-serif" color="black">
	<pt n="serif-bot + 1"/>
      </mv>
    </mv>
    <mv di="top-cv">
      <pt n="top"/>
      <mv di="lc-curved-stem" color="black">
	<pt n="top-ins"/>
      </mv>
    </mv>
    <mv di="longs-knob">
      <pt n="bar-bot"/>
      <mv color="black">
	<pt n="bar-top"/>
      </mv>
    </mv>
  </fn>

  <fn nm="lig-join">
    <pm nm="ref1"/>
    <pm nm="ref2"/>
    <pm nm="pt1"/>
    <pm nm="pt2"/>
    <al>
      <ref><pt n="ref1"/></ref>
      <pt n="pt1"/>
    </al>
    <al>
      <ref><pt n="ref2"/></ref>
      <pt n="pt2"/>
    </al>
  </fn>

  <fn nm="rhotic-hook">
    <pm nm="d-top"/>
    <pm nm="d-bot"/>
    <pm nm="a"/>
    <pm nm="b"/>
    <push>b a d-bot d-top</push>
    <command nm="MDAP" modifier="0"/>
    <command nm="MDAP" modifier="0"/>
    <command nm="MDAP" modifier="1"/>
    <command nm="MDRP">
      <modifier type="color" val="black"/>
    </command>
  </fn>

  <fn nm="lc-dbl-serif">
    <pm nm="first-left"/>
    <pm nm="second-left"/>
    <pm nm="first-right"/>
    <pm nm="second-right"/>
    <pm nm="stem1"/>
    <pm nm="stem2"/>
    <pm nm="cv" val="baseline"/>
    <mv di="cv">
      <pt n="first-left"/>
      <sh>
	<pt n="first-right"/>
	<pt n="stem1"/>
	<pt n="stem2"/>
      </sh>
      <mv di="lc-serif" color="black">
	<pt n="second-left"/>
	<sh><pt n="second-right"/></sh>
      </mv>
    </mv>
  </fn>

  <fn nm="lc-bowl">
    <pm nm="t"/>
    <pm nm="t-ins"/>
    <pm nm="cv-t" val="lc-curved-stem"/>
    <pm nm="b"/>
    <pm nm="b-ins"/>
    <pm nm="cv-b" val="lc-curved-stem"/>
    <mv di="xheight-overshoot">
      <pt n="t"/>
      <mv di="cv-t">
	<pt n="t-ins"/>
      </mv>
    </mv>
    <mv di="baseline-undershoot">
      <pt n="b"/>
      <mv di="cv-b">
	<pt n="b-ins"/>
      </mv>
    </mv>
  </fn>

  <mo nm="shift-rel">
    <pm nm="r"/>
    <pm nm="p"/>
    <push>p r</push>
    <command nm="SRP1"/>
    <command nm="SHP" modifier="1"/>
  </mo>

  <fn nm="interpolated-stem">
    <pm nm="ref-a"/>
    <pm nm="ref-b"/>
    <pm nm="stem-a"/>
    <pm nm="stem-b"/>
    <pm nm="cv"/>
    <ip>
      <ref>
	<pt n="ref-a"/>
	<pt n="ref-b"/>
      </ref>
      <pt n="stem-a"/>
    </ip>
    <mv round="yes">
      <pt n="stem-a"/>
      <mv di="cv" color="black">
	<pt n="stem-b"/>
      </mv>
    </mv>
  </fn>

  <!--
      A macro, since sh-a and sh-b can be points, sets, or
      absent. At the end, RP0, RP2 = pt-b, and RP1 = pt-a
  -->
  <mo nm="serif-sh">
    <pm nm="pt-a"/>
    <pm nm="pt-b"/>
    <pm nm="sh-a" val="-1"/>
    <pm nm="sh-b" val="-1"/>
    <pm nm="cv" val="baseline"/>
    <pm nm="cv-h" val="cap-serif"/>
    <pm nm="align-b" val="-1"/>
    <compile-if test="align-b = 0">
      <mv di="cv">
	<pt n="pt-a"/>
	<sh compile-if="not( nan(sh-a) ) and sh-a &gt;= 0"><pt n="sh-a"/></sh>
	<sh compile-if="nan(sh-a)"><set ref="sh-a"/></sh>
	<mv di="cv-h">
	  <pt n="pt-b"/>
	  <al compile-if="not( nan(sh-b) ) and sh-b &gt;= 0"><pt n="sh-b"/></al>
	  <al compile-if="nan(sh-b)"><set ref="sh-b"/></al>
	</mv>
      </mv>
      <else>
	<mv di="cv">
	  <pt n="pt-a"/>
	  <sh compile-if="not( nan(sh-a) ) and sh-a &gt;= 0"><pt n="sh-a"/></sh>
	  <sh compile-if="nan(sh-a)"><set ref="sh-a"/></sh>
	  <mv di="cv-h">
	    <pt n="pt-b"/>
	    <sh compile-if="not( nan(sh-b) ) and sh-b &gt;= 0"><pt n="sh-b"/></sh>
	    <sh compile-if="nan(sh-b)"><set ref="sh-b"/></sh>
	  </mv>
	</mv>
      </else>
    </compile-if>
  </mo>

  <fn nm="ascender-top">
    <pm nm="top"/>
    <mv di="lc-ascender-top">
      <pt n="top"/>
      <sh>
	<pt n="top - 1"/>
	<pt n="top - 2"/>
      </sh>
    </mv>
  </fn>

  <fn nm="minim-top">
    <pm nm="top"/>
    <mv di="xheight-overshoot">
      <pt n="top"/>
      <sh>
	<pt n="top - 1"/>
	<pt n="top - 2"/>
      </sh>
    </mv>
  </fn>

  <mo nm="lc-horz-stroke">
    <pm nm="pt1"/>
    <pm nm="pt2"/>
    <pm nm="cv"/>
    <push>pt2 pt1 cv</push>
    <command nm="MIAP" modifier="1"/>
    <command nm="MDRP">
      <modifier type="color" val="black"/>
    </command>
  </mo>

  <mo nm="lc-horz-stroke-cv">
    <pm nm="pt1"/>
    <pm nm="pt2"/>
    <pm nm="cv"/>
    <pm nm="cv-h"/>
    <pm nm="codefrag"/>
    <push>pt2 cv-h pt1 cv</push>
    <command nm="MIAP" modifier="1"/>
    <command nm="MIRP">
      <modifier type="color" val="black"/>
    </command>
    <callp nm="codefrag"/>
  </mo>

  <!--
      Makes a stem, rounded to the grid, without consulting
      a cv. At the end, RP0 = pt2, RP1 = pt1 and RP2 = pt2
  -->
  <mo nm="plain-stem">
    <pm nm="pt1"/>
    <pm nm="pt2"/>
    <push>pt2 pt1</push>
    <command nm="MDAP" modifier="1"/>
    <command nm="MDRP">
      <modifier type="color" val="black"/>
    </command>
  </mo>

  <!--
      Like plain-stem, but with a cv for pt1
  -->
  <mo nm="one-cv-stem">
    <pm nm="pt1"/>
    <pm nm="pt2"/>
    <pm nm="cv" val="baseline"/>
    <push>pt2 pt1 cv</push>
    <command nm="MIAP" modifier="1"/>
    <command nm="MDRP">
      <modifier type="color" val="black"/>
      <modifier type="round" val="yes"/>
    </command>
  </mo>

  <!--
      Like plain-stem, but with a cv for the distance
      between pt1 and pt2.
  -->
  <mo nm="one-wd-stem">
    <pm nm="pt1"/>
    <pm nm="pt2"/>
    <pm nm="cv" val="baseline"/>
    <push>pt2 cv pt1</push>
